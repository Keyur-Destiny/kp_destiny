<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_medicine_manufacture_tree" model="ir.ui.view">
        <field name="name">medicine.manufacture.tree</field>
        <field name="model">medicine.manufacture</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="parent_id"/>
            </tree>

        </field>
    </record>

    <record id="view_medicine_manufacture_form" model="ir.ui.view">
        <field name="name">medicine.manufacture.form</field>
        <field name="model">medicine.manufacture</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="parent_id"/>
                    </group>
                </sheet>
            </form>

        </field>
    </record>
</odoo>